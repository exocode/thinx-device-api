{
  "_id": "_design/devicelib",
  "language": "javascript",
  "views": {
    "devices_by_mac": {
      "map": "function (doc) { if(doc.mac) { emit (doc.mac, doc); } }"
    },
    "devices_by_owner": {
      "map": "function (doc) { if(doc.owner) { emit (doc.owner, doc); } }"
    },
    "devices_by_source": {
      "map": "function (doc) { if(doc.source) { emit (doc); } }"
    },
    "devices_by_id": {
      "map": "function (doc) { if(doc.id) { emit (doc); } }"
    },
    "watcher_view": {
      "map": "function (doc) { if(doc.source != null) { emit (doc.owner, doc.device_id, doc.source); } }"
    },
    "devices_by_device_id": {
      "map": "function (doc) { if(doc.device_id) { emit (doc); } }"
    },
    "devices_by_udid": {
      "map": "function (doc) { if(doc.udid) { emit (doc); } }"
    }
  }
}
